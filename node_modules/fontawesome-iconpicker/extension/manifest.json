{
  "manifest_version": 2,

  "name": "Font Awesome Icon Picker",
  "description": "Font Awesome Icon Picker extension for Google Chrome",
  "version": "1.2.2",
  "icons": {
    "19": "icon.png",
    "128": "icon-large.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_security_policy":"script-src 'self' https://netdna.bootstrapcdn.com https://code.jquery.com https://itsjavi.com; object-src 'self'"
}
